    "# last index of one\n",
    "\n",
    "import numpy as np\n",
    "x = np.array([0,0,1,1,0,1,0,1,0])\n",
    "def last_1(x):\n",
    "    index = -1\n",
    "    \n",
    "    for i in range(len(x)):\n",
    "        if x[i] == 1:\n",
    "            index =i\n",
    "    return index\n",
    "print(\"Last occurence 1:\",last_1(x))"
   ]
