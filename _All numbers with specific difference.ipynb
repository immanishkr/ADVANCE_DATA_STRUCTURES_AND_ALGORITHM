    "n=13\n",
    "d=2\n",
    "l=1\n",
    "h=13\n",
    "\n",
    "while l<=h:\n",
    "\n",
    "    m=(l+h)//d\n",
    "    s=sum(int(digit) for digit in str(m))\n",
    "    if (m-s)<=d:\n",
    "        l=m+1\n",
    "    else:\n",
    "        h=m-1\n",
    "\n",
    "x=n-h\n",
    "print(x)\n",
    "        "
